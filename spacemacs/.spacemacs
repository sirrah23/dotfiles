;;Defining org mode templates here
(setq org-capture-templates
      '(("t" "Todo [inbox]" entry
         (file+headline "~/Dropbox/gtd/inbox.org" "Tasks")
         "* TODO %i%?")
        ("T" "Tickler" entry
         (file+headline "~/Dropbox/gtd/tickler.org" "Tickler")
         "* %i%? \n %U")
        ("j" "Journal Entry"
         entry (file+datetree "~/Dropbox/Notes/journal.org")
         "* %?"
         :empty-lines 1)))

